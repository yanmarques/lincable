{
    "name": "yanmarques/lincable",
    "description": "Storage link generator for laravel Eloquent",
    "type": "package",
    "keywords": [
        "laravel", "eloquent", "storage", "link"
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/contracts": "5.5.*|5.6.*",
        "illuminate/support": "5.5.*|5.6.*",
        "illuminate/http": "5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "illuminate/container": "5.5.*|5.6.*",
        "illuminate/database": "5.5.*|5.6.*",
        "illuminate/validation": "5.5.*|5.6.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yan Marques",
            "email": "marques_yan@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lincable\\": "src/Lincable"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Lincable\\": "tests"
        }
    },
    "minimum-stability": "dev"
}

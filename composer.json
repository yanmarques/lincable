{
    "name": "yanmarques/lincable",
    "description": "Storage link generator for laravel Eloquent",
    "type": "package",
    "keywords": [
        "laravel", "eloquent", "storage", "link"
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "~5.7.0",
        "league/flysystem": ">=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.7",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yan Marques",
            "email": "marques_yan@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lincable\\": "src/Lincable"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Lincable\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
